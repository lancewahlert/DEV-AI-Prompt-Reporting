    "templates":[
        {   "parameters": ["BANNER_PARAMETER", "TIMEFRAME_PARAMETER"]
        ,   "desc":"The total sales for BANNER_PARAMETER TIMEFRAME_PARAMETER."
        ,   "questions":["What are the total sales for BANNER_PARAMETER TIMEFRAME_PARAMETER?"]
        ,   "query":"SELECT CONCAT('The total sales for ', A.BANNER_DISPLAY_LABEL, ' in the ', B.TIMEFRAME_DESC ,' timeframe is $', LTRIM(TO_VARCHAR(SUM(OP.NET_SALES_DOLLARS_TY), '999,999,999,990')) ) FROM PC.OPPORTUNITY_LOCATOR OP JOIN PC.LKP_BANNER A ON OP.BANNER_ID = A.BANNER_ID JOIN PC.LKP_TIMEFRAME B ON OP.TIMEFRAME_ID = B.TIMEFRAME_ID WHERE A.BANNER_ID = BANNER_PARAMETER AND B.TIMEFRAME_ID = TIMEFRAME_PARAMETER AND OP.USED_IN_METRICS = TRUE AND STORE_CONCEPT_ID = 1 GROUP BY A.BANNER_DISPLAY_LABEL, B.TIMEFRAME_DESC;"
        ,   "urlpage":"opportunity-locator"
        ,   "urlfilter":""
        ,   "urlquery":"{\"timeframe\":TIMEFRAME_PARAMETER,\"banner\":BANNER_PARAMETER}"
        ,   "urlview":"{\"tab::timeline\":\"departmentDrivers\"}"
        },
        {   "parameters": ["EXCLUDE_DEPARTMENTGROUP_PARAMETER", "TIMEFRAME_PARAMETER", "SALES_GEOGRAPHY_PARAMETER"]
        ,   "desc":"The total sales TIMEFRAME_PARAMETER for SALES_GEOGRAPHY_PARAMETER exluding EXCLUDE_DEPARTMENTGROUP_PARAMETER."
        ,   "questions":["What were my total sales TIMEFRAME_PARAMETER for SALES_GEOGRAPHY_PARAMETER exluding EXCLUDE_DEPARTMENTGROUP_PARAMETER?"]
        ,   "query":"SELECT CONCAT('The total sales in the ', B.TIMEFRAME_DESC ,' timeframe for ', C.SALES_GEOGRAPHY_DISPLAY_LABEL, ' excluding the ', A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL,' department is $', LTRIM(TO_VARCHAR(SUM(OP.NET_SALES_DOLLARS_TY), '999,999,999,990')) ) FROM PC.OPPORTUNITY_LOCATOR OP JOIN PC.LKP_SALES_DEPARTMENT_GROUP A ON A.SALES_DEPARTMENT_GROUP_ID NOT IN (EXCLUDE_DEPARTMENTGROUP_PARAMETER) AND A.SALES_DEPARTMENT_GROUP_ID <> -1 JOIN PC.LKP_TIMEFRAME B ON OP.TIMEFRAME_ID = B.TIMEFRAME_ID JOIN PC.LKP_SALES_GEOGRAPHY C ON OP.SALES_GEOGRAPHY_ID = C.SALES_GEOGRAPHY_ID WHERE OP.SALES_DEPARTMENT_GROUP_ID IN (EXCLUDE_DEPARTMENTGROUP_PARAMETER) AND OP.TIMEFRAME_ID = TIMEFRAME_PARAMETER AND OP.SALES_GEOGRAPHY_ID = SALES_GEOGRAPHY_PARAMETER AND OP.USED_IN_METRICS = TRUE AND STORE_CONCEPT_ID = 1 GROUP BY A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL, B.TIMEFRAME_DESC, C.SALES_GEOGRAPHY_DISPLAY_LABEL;"
        ,   "urlpage":"opportunity-locator"
        ,   "urlfilter":"{\"departmentDrivers\":[EXCLUDE_DEPARTMENTGROUP_PARAMETER],\"salesGeography\":[\"SALES_GEOGRAPHY_PARAMETER\"]}"
        ,   "urlquery":"{\"timeframe\":TIMEFRAME_PARAMETER}"
        ,   "urlview":"{\"tab::timeline\":\"departmentDrivers\"}"
        },
        {   "parameters": ["EXCLUDE_DEPARTMENTGROUP_PARAMETER", "TIMEFRAME_PARAMETER", "STORE_TOP10_PARAMETER"]
        ,   "desc":"The total sales TIMEFRAME_PARAMETER for STORE_TOP10_PARAMETER exluding EXCLUDE_DEPARTMENTGROUP_PARAMETER."
        ,   "questions":["What were my total sales TIMEFRAME_PARAMETER for STORE_TOP10_PARAMETER exluding EXCLUDE_DEPARTMENTGROUP_PARAMETER?"]
        ,   "query":"SELECT CONCAT('The total sales in the ', B.TIMEFRAME_DESC ,' timeframe for ', C.STORE_DISPLAY_SHORT_LABEL, ' excluding the ', A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL,' department is $', LTRIM(TO_VARCHAR(SUM(OP.NET_SALES_DOLLARS_TY), '999,999,999,990')) ) FROM PC.OPPORTUNITY_LOCATOR OP JOIN PC.LKP_SALES_DEPARTMENT_GROUP A ON A.SALES_DEPARTMENT_GROUP_ID NOT IN (EXCLUDE_DEPARTMENTGROUP_PARAMETER) AND A.SALES_DEPARTMENT_GROUP_ID <> -1 JOIN PC.LKP_TIMEFRAME B ON OP.TIMEFRAME_ID = B.TIMEFRAME_ID JOIN PC.LKP_STORE C ON OP.STORE_ID = C.STORE_ID WHERE OP.SALES_DEPARTMENT_GROUP_ID IN (EXCLUDE_DEPARTMENTGROUP_PARAMETER) AND OP.TIMEFRAME_ID = TIMEFRAME_PARAMETER AND OP.STORE_ID = STORE_TOP10_PARAMETER AND OP.USED_IN_METRICS = TRUE AND STORE_CONCEPT_ID = 1 GROUP BY A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL, B.TIMEFRAME_DESC, C.STORE_DISPLAY_SHORT_LABEL;"
